{
  "name": "@impler/client",
  "version": "0.27.0",
  "description": "API client to be used in end user environments",
  "license": "MIT",
  "author": "implerhq",
  "repository": "https://github.com/implerhq/impler.io",
  "types": "dist/index.d.ts",
  "main": "dist/index.js",
  "keywords": [
    "impler",
    "implerhq",
    "data-import",
    "excel-import",
    "csv-import"
  ],
  "files": [
    "dist",
    "package.json"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "start": "npm run start:dev",
    "prebuild": "rimraf dist",
    "start:dev": "npm run watch:build",
    "build": "cross-env node_modules/.bin/tsc -p tsconfig.build.json",
    "fix": "run-s fix:*",
    "fix:prettier": "prettier \"src/**/*.ts\" --write",
    "fix:lint": "eslint src --ext .ts --fix",
    "test:prettier": "prettier \"src/**/*.ts\"",
    "watch:build": "tsc -p tsconfig.json -w"
  },
  "devDependencies": {
    "@types/node": "^18.11.9",
    "rimraf": "^3.0.2",
    "typedoc": "^0.23.20",
    "typescript": "4.8.4"
  },
  "engines": {
    "node": ">=10"
  },
  "prettier": {
    "singleQuote": true
  }
}
