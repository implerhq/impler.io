    "@nestjs/core": "^9.1.2",
    "@nestjs/jwt": "^10.0.1",
    "@nestjs/passport": "^9.0.0",
    "@nestjs/platform-express": "^9.1.2",
    "@nestjs/swagger": "^6.1.2",
    "@nestjs/terminus": "^9.1.3",
    "@sentry/node": "^7.19.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/jsonwebtoken": "^9.0.1",
    "ajv": "^8.11.0",
    "ajv-formats": "^2.1.1",
    "ajv-keywords": "^5.1.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "docker-names": "^1.2.1",
    "dotenv": "^16.0.2",
    "envalid": "^7.3.1",
    "fast-csv": "^4.3.6",
    "hat": "^0.0.3",
    "jsonwebtoken": "^9.0.0",
    "passport": "^0.6.0",
    "passport-github2": "^0.1.12",
    "passport-jwt": "^4.0.1",
    "passport-oauth2": "^1.6.1",
    "rimraf": "^3.0.2",
    "rxjs": "^7.8.0",
    "source-map-support": "^0.5.21",
    "xlsx": "^0.18.5"
