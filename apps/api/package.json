    "@types/express": "^4.17.14",
    "@types/hat": "^0.0.1",
    "@types/mocha": "^10.0.0",
    "@types/multer": "^1.4.7",
    "@types/node": "^18.7.18",
    "@types/passport": "^1.0.11",
    "@types/passport-github2": "^1.2.5",
    "@types/passport-jwt": "^3.0.8",
    "@types/passport-oauth2": "^1.4.11",
    "chai": "^4.3.7",
    "mocha": "^10.1.0",
    "nodemon": "^2.0.20",
    "ts-loader": "^9.4.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.0",
    "typescript": "^4.8.3"
